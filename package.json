{
  "name": "datalog-viewer",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.13.2",
    "@mui/x-data-grid": "6.5.0",
    "@types/node": "20.2.5",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "@types/react-router-dom": "5.3.3",
    "axios": "1.4.0",
    "axios-cache-interceptor": "1.1.1",
    "date-fns": "2.30.0",
    "echarts": "5.4.2",
    "echarts-for-react": "3.0.2",
    "leaflet": "1.9.4",
    "localforage": "1.10.0",
    "match-sorter": "6.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-leaflet": "4.2.1",
    "react-leaflet-tracking-marker": "1.0.16",
    "react-router-dom": "6.11.2",
    "sort-by": "1.2.0",
    "typescript": "4.9.5",
    "web-vitals": "3.3.1"
  },
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --component",
    "lint:check": "eslint .",
    "format:write": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "ignorePatterns": [
      "build/**",
      "coverage/**"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!axios/.*)"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts?(x)",
      "!src/**/*.test.ts?(x)",
      "!src/index.tsx",
      "!src/reportWebVitals.ts",
      "!src/react-app-env.d.ts"
    ],
    "testMatch": [
      "**/?(*.)+(test).ts?(x)"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 1,
        "lines": 1,
        "statements": 1
      }
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.1",
    "@types/leaflet": "1.9.3",
    "axios-mock-adapter": "1.21.4",
    "cypress": "12.13.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-cypress": "2.13.3",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "react-scripts": "5.0.1"
  }
}
